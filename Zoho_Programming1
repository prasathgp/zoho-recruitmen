//using c program

#include<stdio.h>

int main()
{
    int n,c,i,j;

    printf("Enter the number of inputs:");
    scanf("%d",&n);
    c=n;
    for(i=1;i<n;i++)
    {
        for(int j=c;j>c-i;j--)
        {
            printf("%d",j);
        }
        for(j=1; j<=c-1;j++)
        {

            printf("%d",(n-i+1));
        }
        printf("\n");
    }
    return 0;
}
